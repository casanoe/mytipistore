{
  "services": [
    {
      "name": "octoprint",
      "image": "octoprint/octoprint:1.9.3",
      "isMain": true,
      "internalPort": 80,
      "volumes": [
        {
          "host": "${APP_DATA_DIR}/config",
          "container": "/octoprint"
        }
      ],
      "devices": [
        {
          "host": "${SERIAL_PORT}",
          "container": "${SERIAL_PORT}"
        }
      ],
      "environment": {
        "ENABLE_MJPG_STREAMER": "true"
      }
    }
  ]
}