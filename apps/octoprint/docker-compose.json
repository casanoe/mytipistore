{
  "services": [
    {
      "name": "octoprint",
      "image": "octoprint/octoprint:1.9.3",
      "isMain": true,
      "internalPort": 80,
      "volumes": [
        {
          "hostPath": "${APP_DATA_DIR}/config",
          "containerPath": "/octoprint"
        },
        {
          "hostPath": "${APP_DATA_DIR}/data",
          "containerPath": "/data"
        },
        {
          "hostPath": "${ROOT_FOLDER_HOST}/media",
          "containerPath": "/srv/media"
        }        
      ]
    }
  ]
}

